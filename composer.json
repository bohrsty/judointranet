{
    "name": "bohrsty/judointranet",
    "license": "MIT",
    "type": "project",
    "version": "2.1.0",
    "autoload": {
        "psr-4": {
            "": "src/"
        },
        "classmap": [
            "lib/",
            "app/AppKernel.php",
            "app/AppCache.php"
        ],
        "files": [
        	"lib/common.inc.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "repositories": [
        {
            "type": "git",
            "url": "https://github.com/pear/Services_JSON"
        }
    ],
    "require": {
        "php": ">=5.6",
        "symfony/symfony": "3.3.*",
        "doctrine/orm": "^2.5",
        "doctrine/doctrine-bundle": "^1.6",
        "doctrine/doctrine-cache-bundle": "^1.2",
        "doctrine/doctrine-migrations-bundle": "^1.0",
        "symfony/swiftmailer-bundle": "^2.3",
        "symfony/monolog-bundle": "^2.8",
        "symfony/polyfill-apcu": "^1.0",
        "sensio/distribution-bundle": "^5.0",
        "sensio/framework-extra-bundle": "^3.0.2",
        "stefangabos/zebra_form": "2.9.*",
        "phpoffice/phpexcel": "1.8.*",
        "pear/services_json": "dev-trunk",
        "spipu/html2pdf": "4.4.*",
        "smarty/smarty": "3.1.*",
        "setasign/fpdi-fpdf": "1.6.*",
        "friendsofsymfony/user-bundle": "^2.0",
        "sonatra/security-bundle": "1.*@dev",
        "sonatra/security": "1.*@dev",
		"sonatra/doctrine-extensions": "~1.0@dev"

    },
    "require-dev": {
        "sensio/generator-bundle": "^3.0",
        "symfony/phpunit-bridge": "3.*"
    },
    "scripts": {
        "post-install-cmd": [
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::buildBootstrap",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::clearCache",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installAssets",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installRequirementsFile",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::prepareDeploymentTarget",
            "JudoIntranet\\Migrate\\ScriptHandler::createSymlinks"
        ],
        "post-update-cmd": [
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::buildBootstrap",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::clearCache",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installAssets",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installRequirementsFile",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::prepareDeploymentTarget",
            "JudoIntranet\\Migrate\\ScriptHandler::createSymlinks"
        ]
    },
    "extra": {
        "symfony-app-dir": "app",
        "symfony-bin-dir": "bin",
        "symfony-var-dir": "var",
        "symfony-web-dir": "web",
        "symfony-tests-dir": "tests",
        "symfony-assets-install": "relative",
        "symlinks": {
        	"js": "web/js",
        	"css": "web/css",
        	"img": "web/img",
        	"templates": "web/templates",
        	"web/mimes.json": "vendor/stefangabos/zebra_form/mimes.json",
        	"vendor/stefangabos/zebra_form/process.php": "web/process.php",
        	"vendor/stefangabos/zebra_form/public/css": "web/css/zebra_form",
        	"vendor/stefangabos/zebra_form/public/javascript/zebra_form.js": "web/js/zebra_form.js"
        }
    }
}
